# ===========================================================================
# Application specific paths
# ===========================================================================
#
# These applications are listed here because, for one reason or another,
# they require a slightly different path than the basic system path on
# some architecture, or because they need to update some other 
# environment variable.
#
# As much as possible, applications should be installed into a directory 
# _already in the path_ rather than being given their own custom directory.  
# The /software system is set up to make this relatively easy.
#
# Also - these keys should NOT include some path that is otherwise
# included in the path, such as /usr/local/bin or something. 
#
# Please keep these keys in alphabetical order.

(+absoft) {
  [linux] {
    PATH       /soft/com/packages/absoft-1.0/bin
    MANPATH    /soft/com/packages/absoft-1.0/man
    ABSOFT     /soft/com/packages/absoft-1.0
  }
  #[linux-2] {
  #  PATH       /soft/com/packages/absoft-2.0/bin
  #  MANPATH    /soft/com/packages/absoft-2.0/man
  #  ABSOFT     /soft/com/packages/absoft-2.0
  #}
  [linux-2] {
     PATH 	/soft/com/packages/absoft-7.5/opt/absoft/bin
     MANPATH	/soft/com/packages/absoft-7.5/opt/absoft/man
     ABSOFT	/soft/com/packages/absoft-7.5/opt/absoft/
  }
  [linux-rh73] {
     PATH       /soft/com/packages/absoft-7.5/opt/absoft/bin
     MANPATH    /soft/com/packages/absoft-7.5/opt/absoft/man
     ABSOFT     /soft/com/packages/absoft-7.5/opt/absoft/
  }

}

